    <menu name="Icons" inherit="bottom">
      <item name="Github" img="github" href="${project.scm.url}" />
      <item name="Alfresco" img="https://myalfresco.force.com/support/resource/1373366982000/AlfrescoLogo.png" href="http://www.alfresco.com" />
